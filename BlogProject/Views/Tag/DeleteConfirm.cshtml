@model BlogProject.Models.Tag

@{
    ViewBag.Title = "DeleteConfirm";
}


    <div id="containershow">
        <nav>
            <a class="navlink" href="/Tag/Show/@Model.TagId">Cancel</a>
        </nav>
        <h2>Are you sure you want to delete this Tag?</h2>
        <p>@Model.TagName</p>
        <p>This action cannot be undone.</p>
        <form method="POST" action="/Tag/Delete/@Model.TagId">
            <button class="delbutton">
                Confirm Delete
            </button>
        </form>
    </div>



